<h1>Edit Task</h1>

<%= form_with(model: @task, local: true) do |f| %>

  <%= render partial: 'form', locals: {f: f, sections: @project.sections, collaborators: @collaborators} %>

  <%= f.submit "Update Task"%>

<% end %>

<p>Back to <%= link_to @project.title, project_path(@project) %></p>